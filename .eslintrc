{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "2018"
  },
  "env": {
      "browser": true,
      "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
      "indent": ["error", 2]
  },
  "settings": {
    "react":{
      "version": "detect"
    }
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ]
}